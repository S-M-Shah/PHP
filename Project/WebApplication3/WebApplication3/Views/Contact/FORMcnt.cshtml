@model WebApplication3.Models.CONTACT
@{
    ViewBag.Title = "FORMcnt";
    Layout = "~/Views/Shared folder/_Layout1.cshtml";
}



<h2>Contact FORM</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()



    <div class="form-group">
        <label for="inputAddress">User_Name</label>
        @Html.EditorFor(model => model.user_name, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <label for="inputAddress">Cont_num</label>
        @Html.EditorFor(model => model.cont_num, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <label for="inputAddress">Message</label>
        @Html.EditorFor(model => model.message, new { htmlAttributes = new { @class = "form-control" } })
    </div>

    <button type="submit" class="btn btn-primary">Contact Here</button>
}