
@model WebApplication3.Models.Volunteer
@{
    ViewBag.Title = "formV";
    Layout = "~/Views/Shared folder/_Layout1.cshtml";
}


<h2>Volunteer FORM</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()



    <div class="form-group">
        <label for="inputAddress">User_name</label>
        @Html.EditorFor(model => model.user_name, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <label for="inputAddress">CV</label>
        @Html.EditorFor(model => model.cv, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <label for="inputAddress">Email</label>
        @Html.EditorFor(model => model.email, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <label for="inputAddress">Contact</label>
        @Html.EditorFor(model => model.contact, new { htmlAttributes = new { @class = "form-control" } })
    </div>


    <button type="submit" class="btn btn-primary">Sign in</button>
}









