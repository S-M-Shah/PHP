@model IEnumerable<WebApplication3.Models.User>
@{
    ViewBag.Title = "User1";
    Layout = "~/Views/Shared folder/_Layout1.cshtml";
}

<!DOCTYPE html>


<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"></script>
    <style>
        #t3 {
            color: white;
        }
    </style>
</head>
<body>
    <div>

    </div>
    <div class="container mt-3">
        
        <h1 id="t3">user table</h1>

        <table class="table table-dark table-striped">
            <thead>
                <tr>

                    <th>Id</th>
                    <th>users_name</th>
                    <th>Email</th>
                    <th>D_b_o</th>
                    <th>Contact</th>
                    <th>Role_id</th>
                    <th>Edit</th>
                    <th>Delete</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>@Html.DisplayFor(modelitem => item.Id)</td>
                    <td>@Html.DisplayFor(modelitem => item.users_name)</td>
                    <td>@Html.DisplayFor(modelitem => item.Email)</td>
                    <td>@Html.DisplayFor(modelitem => item.d_b_o)</td>
                    <td>@Html.DisplayFor(modelitem => item.contact_num)</td>
                    <td>@Html.DisplayFor(modelitem => item.Role.Roles_name)</td>
                    <td> @Html.ActionLink(" ", "Edit", new { id = item.Id }, new { @class = "fa fa-pencil" })</td>
                    <td> @Html.ActionLink(" ", "Delete", new { id = item.Id }, new { @class = "fa fa-trash" })</td>

                </tr>
                }

            </tbody>
        </table>
    </div>
</body>
</html>
